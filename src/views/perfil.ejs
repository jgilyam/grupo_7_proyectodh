<head>
  <%- include ("./partials/head") %>
  <title>perfil Usuario</title>
  <link rel="stylesheet" href="/css/perfil.css" />
</head>

<body>
  <%- include("./partials/header") %>
  <main>
    <div>
      <section class="section1a">
       <% if (user.imageUsuario != 'default-image.png') { %>
        <img class = "avatar" src="../../img/users/<%= user.imageUsuario %> " alt="avatar" />
       <% }else{%>
       
        <img class = "avatar" src="../../img/users/userAvatar2.png" alt="avatar" />
        <% } %> 
      </section>
      <section>
        <article class="ola">
          <h3>
            <%= user.first_name %>  
            <%= user.last_name %> 
          </h3>
        </article>
        
        <article>
          <h3><%= user.email %></h3>
        </article>
        <article>
          <% if (user.date != '') { %>
            <h3><%=  user.date %> </h3>
          <% }else{ %>
            <h3>no configuro su fecha </h3>
          <% } %> 
        </article>
      </section>
      
    </div>
  </main>

  <%- include ("./partials/footer") %>
</body>
